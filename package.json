{
  "name": "@effectify/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules dist bun.lock && bun pm cache rm & find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name '.output' -o -name '.vinxi' \\) -prune -exec rm -rf {} + & wait"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@nx/esbuild": "21.1.2",
    "@nx/js": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/web": "21.1.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@vitest/ui": "^3.0.0",
    "esbuild": "^0.19.2",
    "jiti": "2.4.2",
    "nx": "21.1.2",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/solid/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
